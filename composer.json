{
    "name": "micheljonkman/laravel-director",
    "version": "0.0.1",
    "description": "This is my package laravel-director",
    "keywords": [
        "MichelJonkman",
        "laravel-director"
    ],
    "homepage": "https://github.com/micheljonkman/laravel-director",
    "license": "MIT",
    "authors": [
        {
            "name": "Michel Jonkman",
            "email": "mj.99@live.nl",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "laravel/framework": "^9.19",
        "inertiajs/inertia-laravel": "^0.6.3"
    },
    "require-dev": {
    },
    "autoload": {
        "psr-4": {
            "MichelJonkman\\Director\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "MichelJonkman\\Director\\Providers\\DirectorServiceProvider"
            ],
            "aliases": {
                "Director": "MichelJonkman\\Director\\Facades\\Director"
            }
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {}
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
